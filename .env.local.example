# Form Integration
# FormSpree endpoint for contact form
# Get your form endpoint from https://formspree.io/
NEXT_PUBLIC_FORMSPREE_CONTACT_ENDPOINT=your_formspree_endpoint_id_here

# Alternative: Custom backend API endpoint
# NEXT_PUBLIC_CONTACT_API_ENDPOINT=https://your-api.com/contact

# Newsletter Integration (optional)
# Email service API key for newsletter subscriptions
NEXT_PUBLIC_NEWSLETTER_API_KEY=your_newsletter_api_key_here
NEXT_PUBLIC_NEWSLETTER_API_ENDPOINT=https://your-newsletter-service.com/subscribe

# Google Analytics (optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
